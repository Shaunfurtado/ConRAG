{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "./dist",
    "rootDir": "../../",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "../../",
    "paths": {
      "*": ["*", "rag/src/*"]
    }
  },
  "include": [
    "src/**/*",
    "../rag/src/**/*"
  ],
  "exclude": ["node_modules"]
}